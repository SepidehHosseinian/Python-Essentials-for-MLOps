#!/home/sepideh/Documents/Source/Repos/Python-Essentials-for-MLOps/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'summarize','console_scripts','summarize'
__requires__ = 'summarize'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('summarize', 'console_scripts', 'summarize')()
    )
